
entity Program {
	name String,
    email String,
	phoneNumber String,
    deploymentRegion Region,
    subdomain String,
    implementationManagerEmail String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity Hierarchy {
	name String,
    workFlow String,
    preferredLanguage Language,
    preferredCurrency Currency,
    primaryCountry Country,
    primaryAddress String,
    secondaryCountry Country,
    secondaryAddress String,
    primaryContactName String,
    primaryContactDesignation String,
    primaryContactEmail String,
    primaryContactPhone String,
    secondaryContactName String,
    secondaryContactDesignation String,
    secondaryContactEmail String,
    secondaryContactPhone String,
    fileUploadPath String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity ProgramUser {
    firstName String,
    lastName String,
    email String,
    sourceId String,
	phoneNumber String,
    clientDesignation String,
    simplifyRole Role,
    fileUploadPath String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity WorkLocation {
	code String,
    name String,
    description String,
    country Country,
    state State,
    address String,
    fileUploadPath String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}


entity FoundationalDataType {
	name String,
    description String,
    requiredInHierarchy Boolean,
    requiredInJobs Boolean,
    requiredInSow Boolean,
    fileUploadPath String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity CustomFieldType {
	name String,
    code String,
    description String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity FoundationalData {
	name String,
    code String,
    description String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}

entity CustomFieldData {
	value String,
    isActive Boolean,
    createdBy String,
    createdAt LocalDate,
    updatedBy String,
    updatedAt LocalDate
}


enum Region {
	Ohio,
    Mumbai
}

enum Language {
	English,
    Spanish,
    Hindi
}

enum Currency {
	USD,
    INR
}
enum Country {
	US,
    India
}

enum Role {
	Administrator,
	Hiring_Manager,
	Finance_Manager,
	Timesheet_Approver,
	Job_Approver
}

enum State {
	Alabama,
	Alaska,
	Arizona,
	Arkansas,
	California,
	Colorado,
	Connecticut,
	Delaware,
	Florida,
	Georgia,
	Hawaii,
	Idaho,
	Illinois,
	Indiana,
	Iowa,
	Kansas,
	Kentucky,
	Louisiana,
	Maine,
	Maryland,
	Massachusetts,
	Michigan,
	Minnesota,
	Mississippi,
	Missouri,
	Montana,
	Nebraska,
	Nevada,
	New_Hampshire,
	New_Jersey,
	New_Mexico,
	New_York,
	North_Carolina,
	North_Dakota,
	Ohio,
	Oklahoma,
	Oregon,
	Pennsylvania,
	Rhode_Island,
	South_Carolina,
	South_Dakota,
	Tennessee,
	Texas,
	Utah,
	Vermont,
	Virginia,
	Washington,
	West_Virginia,
	Wisconsin,
	Wyoming
}

relationship OneToMany {
	Program to Hierarchy{client}
}

relationship OneToMany {
	Program to WorkLocation{client}
}

relationship OneToMany {
	Hierarchy to Hierarchy{parent}
}

relationship ManyToMany {
	Hierarchy{managers} to ProgramUser{hierarchy}
}

relationship OneToMany {
	ProgramUser to ProgramUser{manager}
}

relationship OneToMany {
	WorkLocation to ProgramUser{location}
}


relationship OneToMany {
	Hierarchy to FoundationalDataType{hierarchy}
}

relationship OneToMany {
	FoundationalDataType to CustomFieldType{foundationalDataType}
}

relationship OneToMany {
	FoundationalDataType to FoundationalData{foundationalDataType}
}

relationship ManyToMany {
	FoundationalData{managers} to ProgramUser{foundationalData}
}

relationship OneToMany {
	FoundationalData to CustomFieldData{foundationalData}
    CustomFieldType to CustomFieldData{customFieldType}
}


// Set pagination options
paginate Program, Hierarchy, User, WorkLocation, FoundationalDataType, CustomFieldType, FoundationalData  with infinite-scroll

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

